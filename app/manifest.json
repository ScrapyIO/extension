{
    "name": "__MSG_appName__",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "__MSG_appDescription__",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "permissions":[
        "tabs"
    ],
    "default_locale": "en",
    "content_scripts": [
        {
          "matches": ["http://www.imdb.com/*"],
          "css": ["styles/main.css", "styles/bootstrap.css"],
          "js": ["build.js"],
          "all_frames": true,
          "run_at": "document_start"
        }
    ]
}
